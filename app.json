{
  "expo": {
    "name": "do-it",
    "slug": "do-it",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f6f4fA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mateus.doit",
      "buildNumber": "3.0.0"
    },
    "android": {
      "package": "com.mateus.doit",
      "versionCode": 3,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f6f4fA"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
